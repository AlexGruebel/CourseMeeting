@model CourseMeetingMVC.Models.Course.MyMeetingsViewModel

<h2 class="display-2">My Meetings</h2>

<table class="table">
    <thead>
        <tr>
            <th>MID</th>
            <th>Course</th>
            <th>Teacher</th>
            <th>Meeting Date</th>
            <th>discontinue</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.CourseMeetings){
            <tr id="@item.MID">
                <th>@item.MID</th>
                <th><a asp-controller="Course" asp-action="Course" asp-route-id="@item.Course.CID">@item.Course.CName</a></th>
                <th>@item.Teacher.UserName</th>
                <th>@item.DateOfTheMeeting.ToString("dd.MM.yyyy")</th>
                <th><button MID="@item.MID" class="UnsubsribeAndRemoveRowByMID">unsubscribe</button></th>
            </tr>
        }
    </tbody>
</table>